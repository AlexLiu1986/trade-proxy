<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lecheng.trade.dao.mapper.MemberMapper">
    <insert id="insert" parameterType="com.zb.sms.common.model.SmsSystemMessageDo">
        <selectKey resultType="java.lang.Integer" keyProperty="id" order="AFTER">
            SELECT LAST_INSERT_ID()
        </selectKey>
        insert into sms_system_message
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="sysCode != null">
                sys_code,
            </if>
            <if test="smsCode != null">
                sms_code,
            </if>
            <if test="mobile != null">
                mobile,
            </if>
            <if test="content != null">
                content,
            </if>
            <if test="mobileCount != null">
                mobile_count,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="sysCode != null">
                #{sysCode,jdbcType=VARCHAR},
            </if>
            <if test="smsCode != null">
                #{smsCode,jdbcType=VARCHAR},
            </if>
            <if test="mobile != null">
                #{mobile,jdbcType=LONGVARCHAR},
            </if>
            <if test="content != null">
                #{content,jdbcType=LONGVARCHAR},
            </if>
            <if test="mobileCount != null">
                #{mobileCount,jdbcType=INTEGER},
            </if>
        </trim>
    </insert>
</mapper>